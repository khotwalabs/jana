{
  "name": "@khotwa/jana",
  "version": "0.2.0",
  "description": "Vite plugin for Markdown with Svelte component support and unified plugins",
  "keywords": [
    "jana",
    "markdown",
    "plugin",
    "prose",
    "rehype",
    "remark",
    "svelte",
    "unified",
    "vite"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@vitest/coverage-v8": "^4.0.18",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tsdown": "0.18.4",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "vite": ">=5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.27.0"
}
